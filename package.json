{
  "name": "ignews",
  "private": true,
  "version": "0.1.0",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setupTests.ts"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "test": "jest --env=jsdom",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prismicio/client": "^5.1.0",
    "@stripe/stripe-js": "^1.21.1",
    "axios": "^0.24.0",
    "faunadb": "^4.4.1",
    "next": "12.0.3",
    "next-auth": "^3.29.0",
    "prismic-dom": "^2.2.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "sass": "^1.43.4",
    "stripe": "^8.188.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^16.11.7",
    "@types/prismic-dom": "^2.1.1",
    "@types/react": "^17.0.34",
    "babel-jest": "^28.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.0.3",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^28.0.2",
    "jest-mock": "^28.0.2",
    "typescript": "^4.4.4"
  }
}
